{
  "name": "SCard",
  "type": "registry:ui",
  "dependencies": ["stng-react"],
  "files": ["components/scard/component.tsx"],
  "tailwind": {
    "config": {
      "theme": {
        "extend": {}
      }
    }
  },
  "cssVars": {},
  "meta": {
    "importName": "SCard",
    "exportName": "default"
  },
  "props": {
    "children": {
      "type": "ReactNode",
      "required": true,
      "description": "The content to display inside the card"
    },
    "title": {
      "type": "string",
      "required": false,
      "description": "Optional title for the card"
    },
    "subtitle": {
      "type": "string",
      "required": false,
      "description": "Optional subtitle for the card"
    },
    "variant": {
      "type": "string",
      "enum": ["default", "outlined", "elevated"],
      "default": "default",
      "required": false,
      "description": "Visual variant of the card"
    },
    "padding": {
      "type": "string",
      "enum": ["none", "small", "medium", "large"],
      "default": "medium",
      "required": false,
      "description": "Padding size for the card content"
    },
    "className": {
      "type": "string",
      "required": false,
      "description": "Additional CSS classes"
    },
    "onClick": {
      "type": "function",
      "required": false,
      "description": "Click handler function"
    },
    "header": {
      "type": "ReactNode",
      "required": false,
      "description": "Optional header content"
    },
    "footer": {
      "type": "ReactNode",
      "required": false,
      "description": "Optional footer content"
    }
  }
}
